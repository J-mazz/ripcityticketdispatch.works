{
  "name": "ripcityticketdispatch-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:frontend": "cd rip-city-tickets-react && npm install",
    "install:backend": "cd ripcity-backend && npm install",
    "build:frontend": "cd rip-city-tickets-react && npm run build",
    "build:backend": "cd ripcity-backend && npm run build",
    "start:frontend": "cd rip-city-tickets-react && npm start",
    "start:backend": "cd ripcity-backend && npm start",
    "postinstall": "npm run install:frontend && npm run install:backend",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "npm run start:backend"
  },
  "workspaces": [
    "rip-city-tickets-react",
    "ripcity-backend"
  ]
}